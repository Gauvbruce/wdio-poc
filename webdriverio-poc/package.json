  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "selenium-standalone install",
    "pretest": "rm -rf allure-results",
    "cleanresult": "rm -rf allure-results",
    "test:selenium": "selenium-standalone start",
    "test:mocha": "sleep 1 && multi='progress=- mocha-allure-reporter=-' mocha",
    "test": "node ./node_modules/.bin/wdio ./test/configs/ui/wdio.conf.js",
    "dev": "node_modules/.bin/wdio ./test/configs/ui/wdio.dev.conf.js",
    "feature5": "node_modules/.bin/wdio ./test/configs/ui/wdio.feature5.conf.js",
    "feature4": "node_modules/.bin/wdio ./test/configs/ui/wdio.feature4.conf.js",
    "stag": "node_modules/.bin/wdio ./wdio.stag.conf.js",
    "report": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "createreport": "allure generate allure-results --clean -o allure-report",
    "apitest": "node_modules/.bin/wdio ./test/configs/api/wdio.dev-api.conf.js",
    "apilocal": "node_modules/.bin/wdio ./test/configs/api/wdio.api.conf.js"
  },
